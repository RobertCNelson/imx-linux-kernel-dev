Subject: [PATCH 24/97] apply backport patch
 collateral-evolutions/network/0004-disable-wext-kconfig.patch

---
 backports/net/wireless/Kconfig | 20 +-------------------
 1 file changed, 1 insertion(+), 19 deletions(-)

diff --git a/backports/net/wireless/Kconfig b/backports/net/wireless/Kconfig
index 4f5543d..c8370f5 100644
--- a/backports/net/wireless/Kconfig
+++ b/backports/net/wireless/Kconfig
@@ -1,21 +1,3 @@
-config WIRELESS_EXT
-	bool
-
-config WEXT_CORE
-	def_bool y
-	depends on CFG80211_WEXT || WIRELESS_EXT
-
-config WEXT_PROC
-	def_bool y
-	depends on PROC_FS
-	depends on WEXT_CORE
-
-config WEXT_SPY
-	bool
-
-config WEXT_PRIV
-	bool
-
 config CFG80211
 	tristate "cfg80211 - wireless configuration API"
 	depends on RFKILL || !RFKILL
@@ -177,7 +159,7 @@ config CFG80211_INTERNAL_REGDB
 config CFG80211_WEXT
 	bool "cfg80211 wireless extensions compatibility" if !CFG80211_WEXT_EXPORT
 	depends on CFG80211
-	select WEXT_CORE
+	depends on WEXT_CORE
 	default y if CFG80211_WEXT_EXPORT
 	help
 	  Enable this option if you need old userspace for wireless
